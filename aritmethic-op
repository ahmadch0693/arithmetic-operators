PROBLEM NUMBER : #39

/****************************************************************************************/
/****************************************************************************************/


#include <iostream>
using namespace std;

int main()

{
	int TotalBill;
	int CashPaid;


	cout << endl;

	cout << "Enter the cash paid " << endl;

	cout << endl;

	cin >> CashPaid;

	

	cout << endl;

	cout << "Enter the total bill " << endl;

	cout << endl;

	cin >> TotalBill;


	cout << endl;


	int Remainder = TotalBill - CashPaid;

	cout << endl;

	cout << Remainder;

	cout << endl;


	return 0;
}


