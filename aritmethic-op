PROBLEM NUMBER : #19

/****************************************************************************************/
/****************************************************************************************/

int main()

{

	int D;

	int PI = 3.14;
	

	cout << endl;

	cout << "Please enter the number  " << endl;
	 
	cout << endl;
 
	cin >> D ;


	cout << endl;

	int Area =  (PI * D * D) / 4;

	cout << Area; 


	
	return 0;
}




