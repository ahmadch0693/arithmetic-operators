#include <iostream>
using namespace std;

int main()

{

	int Base;
	int Height;
	

	cout << endl;

	cout << "Please enter the base " << endl;
	 
	cout << endl;
 
	cin >> Base ;

	cout << endl;

	cout << "Please enter the height " << endl;

	cout << endl;

	cin >> Height;

	cout << endl;

	int Area =  Base/2 * Height;

	cout << Area; 

	
	return 0;
}

