/****************************************************************************************/



#include <iostream>
using namespace std;

int main()

{

	int a;

	
	
	cout << endl;

	cout << "Please enter a  number " << endl;
	 
	cout << endl;
 
	cin >> a ;

	cout << endl;
	
	int Product1 = a * a;

	cout << Product1;


	cout << endl;

	int Product2 = a * a * a;

	cout << Product2;


	cout << endl;

	int Product3 = a * a * a * a;

	cout << Product3;

	cout << endl;

	
	
	return 0;
}


/****************************************************************************************/
/****************************************************************************************/

PROBLEM NUMBER : #35

/****************************************************************************************/
/****************************************************************************************/


#include <iostream>
using namespace std;

int main()

{
	int Penny = 1;
	int Nickel = Penny * 5; 
	int Dime  = Penny * 10;
	int Quarter = Penny * 25;
	int Dollar = Penny * 100;

	
	cout << endl;

	cout << "Please enter the Penny's number " << endl;

	cout << endl;

	cin >> Penny;

	cout << endl;

	cout << "Please enter Nickel's number " << endl;

	cout << endl;

	cin >> Nickel;

	cout << endl;

	cout << endl;

	cout << "Please enter Dime's number " << endl;

	cout << endl;

	cin >> Dime;

	cout << endl;

	cout << endl;

	cout << "Please enter Quarter's number " << endl;

	cout << endl;

	cin >> Quarter;

	cout << endl;

	cout << endl;

	cout << "Please enter Dollar's number " << endl;

	cout << endl;

	cin >> Dollar;

	cout << endl;


	int TotalPenny = (1 + 5 + 10 + 25 + 100) * 5;

	cout << TotalPenny;

	cout << endl;

	int TotalDollar = TotalPenny / 100;

	cout << TotalDollar;


	return 0;
}

