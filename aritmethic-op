#include <iostream>
using namespace std;

int main()

{

	int a;

	cout << endl;

	cout << "Please enter a number " << endl;
	 
	cout << endl;
 
	cin >> a ;

	cout << endl;

	int quotient = a / 2;

	cout << endl;

	cout << quotient << endl;

	cout << endl;



	
	return 0;
}


