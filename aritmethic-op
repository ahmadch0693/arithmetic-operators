PROBLEM NUMBER : #9 

/****************************************************************************************/
/****************************************************************************************/

#include <iostream>
using namespace std;

int main()

{

	int a, b, c;

	cout << endl;

	cout << "Please enter the first number  " << endl;
	 
	cout << endl;
 
	cin >> a ;

	cout << endl;

	cout << "Please enter the second  number  " << endl;

	cout << endl;

	cin >> b;


	cout << "Please enter the third  number  " << endl;

	cout << endl;

	cin >> c;

	int sum  = a + b + c;

	cout << endl;

	cout << sum << endl;

	cout << endl;



	
	return 0;
}



