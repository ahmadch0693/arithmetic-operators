PROBLEM NUMBER : #47

/****************************************************************************************/
/****************************************************************************************/


#include <iostream>
using namespace std;

int main()

{
	int LoanAmount;
	int MonthlyPayment;


	cout << endl;

	cout << "Enter the loan amount " << endl;

	cout << endl;

	cin >> LoanAmount;

	cout << endl;

	cout << "Enter the monthly payment " << endl;

	cout << endl;

	cin >> MonthlyPayment;

	cout << endl;

	int NumberOfMonths = LoanAmount / MonthlyPayment;

	cout << endl;

	cout << NumberOfMonths;


	cout << endl;



	return 0;
}



