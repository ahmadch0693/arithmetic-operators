#include <iostream>
using namespace std;

int main()

{

	int L;

	int PI = 3.14;
	

	cout << endl;

	cout << "Please enter the number  " << endl;
	 
	cout << endl;
 
	cin >> L ;


	cout << endl;

	int Area =  (L * L ) / (4 * PI);

	cout << Area; 


	
	return 0;
}



/****************************************************************************************/
/****************************************************************************************/

PROBLEM NUMBER : #22

/****************************************************************************************/
/****************************************************************************************/


#include <iostream>
using namespace std;

int main()

{

	int a, b;

	int PI = 3.14;



    cout << endl;

	cout << "Please enter the first number " << endl;
	 
	cout << endl;
 
	cin >> a ;

	cout << endl;
	
	cout << "Please enter the second number " << endl;
	
	cout << endl;

	cin >> b;

	cout << endl;

	int Area = ((PI * b * b / 4) * ((2 * a - b)) / (2 * a + b));

	cout << Area; 

	return 0;
}




