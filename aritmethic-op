
#include <iostream>
using namespace std;

int main()

{
	int a, b;

	cout << "Please enter a number " << endl;

	cin >> a;

	cout << endl;

	cout << "Please enter the second number " << endl;
	cin >> b;


	int sum = a + b;

	cout << endl;

	cout << sum << endl;

	int difference = a - b;

	cout << endl;

	cout << difference << endl;

	int product = a * b;

	cout << endl;

	cout << product << endl;


	int quotient = a / b;

	cout << endl;

	cout << quotient << endl;

	int modulo = a % b;


	cout << endl;

	cout << modulo << endl;

