#include <iostream>
using namespace std;

int main()

{

	int A;

	int PI = 3.14;
	

	cout << endl;

	cout << "Please enter the number  " << endl;
	 
	cout << endl;
 
	cin >> A ;


	cout << endl;

	int Area =  (PI * (A * A) ) / 4;

	cout << Area; 


	
	return 0;
}



