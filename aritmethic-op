PROBLEM NUMBER : #48

/****************************************************************************************/
/****************************************************************************************/



#include <iostream>
using namespace std;

int main()

{
	int LoanAmount;
	int MonthlyInstallement;


	cout << endl;

	cout << "Enter the loan amount " << endl;

	cout << endl;

	cin >> LoanAmount;

	cout << endl;

	cout << "Enter the month installement " << endl;

	cout << endl;

	cin >> MonthlyInstallement;

	cout << endl;

	int AmoutPerMonth = LoanAmount / MonthlyInstallement;

	cout << endl;

	cout << AmoutPerMonth;


	cout << endl;



	return 0;
}


