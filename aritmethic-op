/****************************************************************************************/

PROBLEM NUMBER : #42



#include <iostream>
using namespace std;

int main()

{

	int NumberOfDays;
	int NumberOfHours;
	int NumberOfMinutes;
	int NumberOfSeconds;


	cout << endl;

	cout << "Enter the days numnber " << endl;

	cout << endl;

	cin >> NumberOfDays;


	cout << endl;

	cout << "Enter the hours number " << endl;

	cout << endl;

	cin >> NumberOfHours;

	cout << endl;

	cout << "Enter the minutes number " << endl;

	cout << endl;

	
	cin >> NumberOfMinutes;

	cout << endl;

	cout << "Enter the seconds number " << endl;

	cout << endl;

	cin >> NumberOfSeconds;

	cout << endl;


	int TotalSeconds = (NumberOfDays * 24 * 60 * 60) + (NumberOfHours * 60 * 60) + (NumberOfMinutes * 60) + NumberOfSeconds;


	cout << endl;


	cout << TotalSeconds;

	cout << endl;




	return 0;
}





/****************************************************************************************/
/****************************************************************************************/

PROBLEM NUMBER : #43

/****************************************************************************************/
/****************************************************************************************/


#include <iostream>
using namespace std;

int main()

{
	int TotalSeconds = 193535;
	int SecondsPerDay = 24*60*60;
	int SecondsPerHour = 60*60;
	int SecondsPerMinutes = 60;

	cout << endl;

	int NumberOfDays = TotalSeconds / SecondsPerDay;

	cout << endl;


	cout << endl;

	int DaysRemainder = TotalSeconds % SecondsPerDay;

	cout << endl;

	int NumberOfHours = DaysRemainder / SecondsPerHour;
	
	cout << endl;


	cout << endl;

	int HoursRemainder  =  DaysRemainder % SecondsPerHour;

	cout << endl;

	

	int NumberOfMinutes = HoursRemainder / SecondsPerMinutes;

	cout << endl;


	cout << endl;

	int NumberOfSeconds = HoursRemainder % SecondsPerMinutes;

	cout << endl;



	cout << NumberOfDays << ": " << NumberOfHours << ": " << NumberOfMinutes << ": " << NumberOfSeconds << endl;

	cout << endl;


	return 0;
}


